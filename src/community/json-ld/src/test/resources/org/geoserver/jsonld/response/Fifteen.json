{
  "@context": {
    "gsp": "http://www.opengis.net/ont/geosparql#",
    "sf": "http://www.opengis.net/ont/sf#",
    "schema": "https://schema.org/",
    "dc": "http://purl.org/dc/terms/",
    "Feature": "gsp:Feature",
    "FeatureCollection": "schema:Collection",
    "Point": "sf:Point",
    "wkt": "gsp:asWKT",
    "features": {
      "@container": "@set",
      "@id": "schema:hasPart"
    },
    "geometry": "sf:geometry",
    "description": "dc:description",
    "title": "dc:title",
    "name": "schema:name"
  },
  "type": "FeatureCollection",
  "features": [
    {
      "$source": "cdf:Fifteen"
    },
    {
      "id": "${@id}",
      "@type": [
        "Feature",
        "cdf:Fifteen",
        "http://vocabulary.odm2.org/samplingfeaturetype/fifteen"
      ],
      "boundedBy": "$${toWKT(xpath('cdf:boundedBy'))}",
      "geometry": {
        "@type": "Polygon",
        "wkt": "$${toWKT(xpath('cdf:pointProperty'))}"
      }
    }
  ]
}
